{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonJS",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./src",
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
